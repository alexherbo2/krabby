{
  "manifest_version": 2,
  "name": "Krabby",
  "description": "A keyboard interface to the web",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "packages/modal.js",
        "packages/prompt.js",
        "packages/hint.js",
        "packages/mark.js",
        "packages/selection.js",
        "packages/mouse.js",
        "packages/clipboard.js",
        "packages/scroll.js",
        "packages/player.js",
        "packages/krabby/web/krabby.js",
        "packages/krabby/extension/extension.js",
        "packages/krabby/extension/init.js",
        "config.js"
      ],
      "css": [
        "packages/selection.css",
        "packages/krabby/web/krabby.css"
      ]
    }
  ],
  "icons": {
    "16": "packages/krabby.png",
    "48": "packages/krabby.png",
    "128": "packages/krabby.png"
  }
}
